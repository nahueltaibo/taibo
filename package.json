    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.13.1",
    "@types/react-helmet": "^6.1.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "^10.1.0",
