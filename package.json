    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^26.2.2",
    "npm-check": "^6.0.1",
    "prettier": "^2.8.4",
    "react-markdown": "^4.3.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
